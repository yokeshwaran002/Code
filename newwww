def emp():
    __name__=input("Enter your name:")
    __age__=int(input("Enter your age:"))
    __place__=input("Enter your place:")
    __mobile_number__=int(input("Enter your mobile number:"))
    __10mark__,__10mark__=int(input("Enter your '10Mark:")),int(input("Enter your '12Mark:"))
    print("Name:",__name__)
    print("Age:",__age__)
    print("Place:",__place__)
    print("Mobile Number:",__mobile_number__)
    print("10th Mark:",__10mark__)
    print("12th Mark:",__10mark__)
    print(f"this is my name {__name__}")
    



